<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="gridpane" stylesheets="@style.css" hgap="15.0" prefHeight="480.0" prefWidth="640.0" vgap="15.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.vizsgaremek_javafx.InsertfoodController">
   <columnConstraints>
      <ColumnConstraints halignment="RIGHT" minWidth="75.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="TOP" vgrow="SOMETIMES" />
   </rowConstraints>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
   <children>
      <Label text="Étel neve" />
      <Label text="Kalória" GridPane.rowIndex="1" />
      <Label text="Protein" GridPane.rowIndex="3" />
      <TextField fx:id="nameField" GridPane.columnIndex="1" />
      <TextField fx:id="calorieField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <TextField fx:id="carboField" layoutX="115.0" layoutY="25.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <TextField fx:id="proteinField" layoutX="115.0" layoutY="25.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <TextField fx:id="fatField" layoutX="140.0" layoutY="158.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Button fx:id="Feltölt" mnemonicParsing="false" onAction="#submitClick" prefWidth="10000.0" text="Felvétel" GridPane.columnSpan="2" GridPane.rowIndex="5" GridPane.valignment="CENTER" />
      <Label text="Szénhidrát" GridPane.rowIndex="2" />
      <Label text="Zsír" GridPane.rowIndex="4" />
   </children>
</GridPane>
